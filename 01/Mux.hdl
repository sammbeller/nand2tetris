/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=a, b=sel, out=s1);
    Not(in=sel, out=notsel);
    And(a=b, b=notsel, out=s2);
    Or(a=s1, b=s2, out=out);
}